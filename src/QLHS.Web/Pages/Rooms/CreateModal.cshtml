@page
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Form
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@model QLHS.Web.Pages.Rooms.CreateModal

@{
    Layout = null;
}

<abp-dynamic-form abp-model="Room" asp-page="/Rooms/CreateModal">
    <abp-modal>
        <abp-modal-header title="New Room"></abp-modal-header>
        <abp-modal-body>
            <abp-form-content />
        </abp-modal-body>
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</abp-dynamic-form>